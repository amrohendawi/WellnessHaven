{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "framework": "vite",
  "routes": [
    { "src": "/api/admin/(.*)", "dest": "/api/admin/$1", "methods": ["GET", "POST", "PUT", "DELETE"] },
    { "src": "/api/(.*)", "dest": "/api/$1", "methods": ["GET", "POST", "PUT", "DELETE"] },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
